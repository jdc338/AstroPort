<h1>My Spaceships</h1>

<% @spaceships.each do |spaceship| %>
  <div class="card m-4">
    <h2><%= spaceship.name %></h2>
      <%# <%= cl_image_path spaceship.photo.key height: '150', width: '200' %> %>
      <h4>Description: <%= spaceship.description %></h4>
      <h4>Price: <%= spaceship.price %></h4>
    <%= link_to "Update Info", spaceship_path(spaceship), class: "btn btn-primary" %>
  </div>
<% end %>


